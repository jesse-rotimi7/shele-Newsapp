<svg width="512" height="512" viewBox="0 0 512 512" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="roseGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e11d48;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#be123c;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="8" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background rounded square -->
  <rect width="512" height="512" rx="110" fill="url(#roseGradient)"/>
  
  <!-- Highlight overlay -->
  <rect x="30" y="30" width="452" height="452" rx="95" fill="white" opacity="0.1"/>
  
  <!-- Newspaper icon with glow -->
  <g filter="url(#glow)">
    <!-- Bold header -->
    <rect x="120" y="135" width="272" height="32" rx="16" fill="white" opacity="0.95"/>
    
    <!-- Two column layout -->
    <rect x="120" y="195" width="125" height="22" rx="11" fill="white" opacity="0.92"/>
    <rect x="267" y="195" width="125" height="22" rx="11" fill="white" opacity="0.92"/>
    
    <rect x="120" y="235" width="125" height="22" rx="11" fill="white" opacity="0.92"/>
    <rect x="267" y="235" width="125" height="22" rx="11" fill="white" opacity="0.92"/>
    
    <rect x="120" y="275" width="125" height="22" rx="11" fill="white" opacity="0.92"/>
    <rect x="267" y="275" width="125" height="22" rx="11" fill="white" opacity="0.92"/>
    
    <!-- Full width lines -->
    <rect x="120" y="325" width="272" height="16" rx="8" fill="white" opacity="0.8"/>
    <rect x="120" y="357" width="272" height="16" rx="8" fill="white" opacity="0.8"/>
    <rect x="120" y="389" width="272" height="16" rx="8" fill="white" opacity="0.75"/>
  </g>
</svg>


